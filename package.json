{
  "name": "swarmdb-demo",
  "version": "1.3.1",
  "private": true,
  "homepage": "http://olebedev.github.io/chat",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "./cp.sh",
    "test": "jest",
    "start:web": "node web/scripts/start.js",
    "build:web": "node web/scripts/build.js && mv ./web/build/index.html ./web/build/app.html && mv ./web/build/iframe.html ./web/build/index.html && rm ./web/build/test.html",
    "deploy:web": "gh-pages -d web/build",
    "version": "./version-ios.sh"
  },
  "dependencies": {
    "auth0-lock": "^11.3.1",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "es6-iterator": "^2.0.3",
    "es6-symbol": "^3.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.1.0",
    "graphql": "^0.13.1",
    "graphql-anywhere": "^4.1.6",
    "graphql-tag": "^2.8.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "moment": "^2.21.0",
    "object-assign": "4.1.1",
    "object-hash": "^1.2.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "16.2.0",
    "react-native": "0.53.2",
    "react-native-fast-image": "^4.0.0",
    "react-native-gifted-chat": "^0.4.1",
    "react-native-iphone-x-helper": "^1.0.2",
    "react-native-web": "^0.5.2",
    "react-native-web-lists": "^0.1.1",
    "react-navigation": "^1.1.2",
    "regenerator-runtime": "^0.11.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "flow-bin": "0.63.0",
    "jest": "22.3.0",
    "jwt-decode": "^2.2.0",
    "react-native-auth0": "^1.2.2",
    "react-native-flip-card": "^3.5.2",
    "react-test-renderer": "16.2.0",
    "url-parse": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
